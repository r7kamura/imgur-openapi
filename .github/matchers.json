{
  "problemMatcher": [
    {
      "owner": "redocly-openapi-lint",
      "pattern": [
        {
          "regexp": "^([^\\s].*):$",
          "file": 1
        },
        {
          "regexp": "^\\s+(\\d+):(\\d+)\\s+(error|warning)\\s+(.*)\\s\\s+(.*)$",
          "line": 1,
          "column": 2,
          "severity": 3,
          "code": 4,
          "message": 5,
          "loop": true
        }
      ]
    }
  ]
}
